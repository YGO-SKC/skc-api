{
	"configuration": {
		"packages": "com.rtomyj",
		"appenders": {
			"Console": {
				"name": "STD_Out",
				"PatternLayout": {
					"pattern": "[ %d{YYYY/dd/MM HH:mm:ss.SSS} ] [%t] %-5p %c: %m%n"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "info",
				"AppenderRef": {
					"ref": "STD_Out"
				}
			}
		}
	}
}