{
	"configuration": {
		"packages": "com.rtomyj",
		"appenders": {
			"Console": {
				"name": "STD_Out",
				"PatternLayout": {
					"pattern": "%-5p: @[ %d{YYYY/dd/MM HH:mm:ss} ] [ w/ info: reqIp=%X{reqIp}, reqRes=%X{reqRes} ] [ %t ] %c: %m%n"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "info",
				"AppenderRef": {
					"ref": "STD_Out"
				}
			}
		}
	}
}