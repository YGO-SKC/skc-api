{
	"configuration": {
		"packages": "com.rtomyj",
		"appenders": {
			"Console": {
				"name": "STD_Out",
				"PatternLayout": {
					"pattern": ">>> %-5p: [ %d{YYYY/dd/MM HH:mm:ss} ] [ reqIp=%X{reqIp}, reqUrl=%X{reqUrl}, reqUUID=%X{reqUUID}, clientUUID=%X{clientUUID} ] [ %l ] %m%n"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "info",
				"AppenderRef": {
					"ref": "STD_Out"
				}
			}
		}
	}
}